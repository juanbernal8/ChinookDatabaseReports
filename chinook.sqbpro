axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="130"/><column index="2" value="300"/><column index="3" value="141"/><column index="4" value="202"/><column index="5" value="136"/><column index="6" value="300"/><column index="7" value="190"/><column index="8" value="127"/><column index="9" value="149"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">SELECT customers.Country, count(customers.CustomerId)
FROM customers
GROUP BY customers.Country
ORDER BY 2 DESC, 1 </sql><sql name="Q1">-- clientes por pa√≠s
SELECT Country, count(CustomerId) as CustomersCount
FROM customers
GROUP BY Country
ORDER BY CustomersCount DESC</sql><sql name="Q2 - Top songs by sales">SELECT tracks.Name as 'Song', artists.Name as 'Artist', albums.Title as 'Album', sum(invoices.total) as TotalSales
FROM invoices
INNER JOIN invoice_items
	ON invoices.InvoiceId = invoice_items.InvoiceId
INNER JOIN tracks
	ON invoice_items.TrackId = tracks.TrackId
INNER JOIN albums 
	ON tracks.AlbumId = albums.AlbumId
INNER JOIN artists
	ON albums.ArtistId = artists.ArtistId
GROUP BY invoice_items.TrackId
ORDER BY TotalSales DESC
LIMIT 100</sql><sql name="Q3 - Price of each artist ">SELECT artists.Name as 'ArtistName', count(tracks.TrackId) as 'TotalSongs' ,sum(tracks.UnitPrice) as 'TotalPrice'
FROM tracks
INNER JOIN albums
	ON tracks.AlbumId = albums.AlbumId
INNER JOIN artists
	ON albums.ArtistId = artists.ArtistId
GROUP BY artists.name
HAVING TotalSongs &gt; 5
ORDER BY TotalPrice DESC
</sql><sql name="SQL 9">SELECT *
FROM artists
WHERE artists.name = 'Pink Floyd'</sql><sql name="Q4 - Songs by Genre">SELECT genres.Name, count(tracks.TrackId) as 'Songs'
FROM tracks
INNER JOIN genres
	ON tracks.GenreId = genres.GenreId
GROUP BY genres.Name
ORDER BY Songs DESC</sql><sql name="SQL 13">SELECT count(tracks.TrackId)
FROM tracks
WHERE tracks.GenreId = 1</sql><sql name="Q5  - Sales">SELECT CAST(substr(invoices.InvoiceDate,1,4) as TEXT) as 'Year', sum(invoices.total) as 'Sales'
FROM invoices
GROUP BY Year</sql><sql name="Q6 - Salestrend">SELECT substr(invoices.InvoiceDate,1,4) as 'Year', substr(invoices.InvoiceDate,6,2) as 'Month', sum(invoices.total) as 'Sales'
FROM invoices
GROUP BY Year, Month
</sql><sql name="Q6 - Salestrend (1)">SELECT substr(invoices.InvoiceDate,1,4) || '-' || substr(invoices.InvoiceDate,6,2) as 'Month', sum(invoices.total) as 'Sales'
FROM invoices
GROUP BY Month
</sql><sql name="SQL 16">SELECT substr(invoices.InvoiceDate,1,4) as 'Year', substr(invoices.InvoiceDate,6,2) as 'Month'
FROM invoices
--GROUP BY Year</sql><sql name="Q4 - Songs by Genre (1)">SELECT genres.Name, count(tracks.TrackId) as 'Songs'
FROM tracks
INNER JOIN genres
	ON tracks.GenreId = genres.GenreId
GROUP BY genres.Name
ORDER BY Songs DESC</sql><current_tab id="11"/></tab_sql></sqlb_project>
